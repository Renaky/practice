#include<stdio.h>

int main(void){
	
	float peso, altura, imc;
	

		printf("Insira seu peso:  ");
	scanf("%f",&peso);
	printf("\nInsira sua altura: ");
	scanf("%f",&altura);
	
	
	imc = peso/(altura*altura);  //formula de calculo imc
		
	if(imc<20){
		printf("\nIMC= %f. Abaixo do peso",imc);
	}
	if(imc>=20 && imc<=25){
		printf("\nIMC = %f. Peso Normal",imc);
	}
	if(imc>25 && imc<30){
		printf("\nIMC = %f. Sobre Peso",imc);
	}
	if(imc>=30 && imc<40){
		printf("\nIMC = %f. Obeso",imc);
		
	}if (imc>=40){
		printf("\nIMC = %f. Obeso Morbido",imc);
	}
	}
