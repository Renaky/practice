#include <stdio.h>

int main(void){
	
	float num1, num2;
	printf("digite dois numeros, um apos o outro : \n");
	scanf("%f",&num1);
	scanf("%f",&num2);
	
	if(num1>num2){   //condicao de quais numeros sao maiores
		printf("\n%2.f e o maior numero entre os dois",num1);
		
	}else{  //segunda condicao de quais numeros sao maiores
		printf("\n%2.f e o maior numero entre os dois",num2);
	
	}

	
}
