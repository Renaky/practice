#include<stdio.h>

int main(){
	
	char nomeTime1[50], nomeTime2[50];
	int gols1, gols2, vitoria=3,empate=1,derrota=0;
	
	printf("Insira o nome do time 1:\n");
	scanf("%s",&nomeTime1);
	printf("Insira o nome do time 2:\n");
	scanf("%s",&nomeTime2);
	printf("Quantos Gols fez o time %s: \n",nomeTime1);
	scanf("%d",&gols1);
	printf("\nQuantos Gols fez o time %s: \n",nomeTime2);
	scanf("%d",&gols2);
	
	if(gols1 > gols2){
	
		printf("\nVitoria do time %s ele recebe %d ponto(s)",nomeTime1,vitoria);
}
	if(gols2>gols1){
		printf("\nVitoria do time %s ele recebe %d ponto(s)", nomeTime1,vitoria);
	}
	if(gols1==gols2){
		printf("\nEmpate!!! ambos times recebem %d ponto(s)",empate);
	}
	if(gols1<gols2){
		printf("\nDerrota, o time %s recebe %d ponto(s)",nomeTime1,derrota);
			
	}
	if(gols2<gols1){
		printf("\nDerrota, o time %s recebe %d ponto(s)",nomeTime2,derrota);
	}
}
